{
  "name": "vue3-boot",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "npm run type:check && npm run lint && vite build",
    "build:test": "npm run type:check && npm run lint && vite build --mode test",
    "preview": "vite preview",
    "type:check": "vue-tsc --noEmit",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --write \"src/**/*.{js,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "lint:eslint": "eslint --cache \"src/**/*.{js,ts,tsx,vue}\""
  },
  "dependencies": {
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/node": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "prettier": "2.5.1",
    "sass": "^1.45.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.2",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.29.8"
  }
}
