{
  "name": "vue3-boot",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:xxx": "cross-env CLIENT=xxx npm run dev",
    "build": "vite build",
    "build:test": "vite build --mode test",
    "preview": "vite preview",
    "type:check": "vue-tsc",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:prettier": "prettier --write \"src/**/*.{js,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "lint:eslint": "eslint --cache --fix \"src/**/*.{js,ts,tsx,vue}\" \"types/**/*.{d.ts,ts}\"",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "pinia": "^2.0.33",
    "pinia-plugin-persistedstate": "^3.1.0",
    "qs": "^6.11.1",
    "store2": "^2.14.2",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.6",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.15.1",
    "vite": "^4.2.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,json,ts,tsx,css,less,scss,vue,html,md}": "prettier --write",
    "src/**/*.{js,ts,tsx,vue}": "eslint --cache --fix",
    "types/**/*.{d.ts,ts}": "eslint --cache --fix"
  }
}
